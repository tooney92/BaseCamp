

<div class="card">
    <div class="card-header">
      <h1>Title: <%= @project.title %> </h1>
    </div>
    <div class="card-body">
      <h5 class="card-title"><p>Created: <%= time_ago_in_words(@project.created_at)  %> ago </p></h5>
      <p class="card-text"><%= @project.body %></p>
      <%= link_to "back",  user_projects_path, :class => "btn btn-primary" %>
    </div>
</div>